{
  "name": "parker-temporary",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "build:dev": "pnpm install && pnpm db:migrate && pnpm db:migrate:test && pnpm build && pnpm format && pnpm lint",
    "clean": "pnpm db:clean && turbo run clean && rm -rf node_modules/",
    "db:clean": "docker rm -f -v parker_postgres_dev parker_postgres_test && docker volume rm -f parker_postgres_data_dev parker_postgres_data_test",
    "db:migrate": "./tools/scripts/ensure_postgres_up.sh && turbo run db:migrate",
    "db:migrate:test": "./tools/scripts/ensure_postgres_up.sh -p 5441 -c parker_postgres_test -v parker_postgres_data_test && turbo run db:migrate:test",
    "dev": "turbo run dev --parallel",
    "format": "prettier --write \"**/*.{ts,tsx,md,js,jsx,json}\" && turbo run format",
    "generate": "turbo run generate",
    "lint": "prettier --check \"**/*.{ts,tsx,md,js,jsx,json}\" && turbo run lint",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch"
  },
  "devDependencies": {
    "prettier": "latest",
    "turbo": "latest"
  },
  "dependencies": {},
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.14.1"
}
