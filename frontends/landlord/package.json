{
  "name": "@parker/landlord",
  "version": "0.0.1",
  "private": true,
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf .expo/ .turbo/ dist/ coverage/ node_modules/",
    "serve": "yarn serve:ios",
    "serve:android": "expo start --android",
    "serve:ios": "expo start --ios",
    "serve:start": "expo start",
    "serve:web": "expo start --web",
    "eject": "expo eject",
    "format": "eslint . --ext .ts,.tsx --fix",
    "lint": "tsc --noEmit && eslint . --ext .ts,.tsx",
    "test": "LOG_LEVEL=\"${LOG_LEVEL:-off}\" jest src/ --runInBand --passWithNoTests",
    "test:watch": "LOG_LEVEL=\"${LOG_LEVEL:-off}\" jest src/ --watch --runInBand --passWithNoTests"
  },
  "dependencies": {
    "@ory/kratos-client": "^1.0.0",
    "@parker/core-client": "*",
    "@react-native-async-storage/async-storage": "1.18.2",
    "axios": "^1.5.0",
    "expo": "^49.0.11",
    "expo-secure-store": "~12.3.1",
    "expo-status-bar": "~1.6.0",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.4",
    "react-native-flash-message": "^0.4.2",
    "react-native-url-polyfill": "^2.0.0",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.22.15",
    "@types/react": "^18.0.22",
    "@types/react-native": "^0.72.2",
    "eslint": "8.49.0",
    "eslint-config-custom": "*",
    "jest": "^29.0.3",
    "ts-jest": "^29.0.3",
    "typescript": "^5.2.2"
  }
}
