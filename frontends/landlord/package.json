{
  "name": "@parker/landlord",
  "version": "0.0.1",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "clean": "rm -rf .expo/ .turbo/ dist/ coverage/ node_modules/",
    "serve": "yarn serve:ios",
    "serve:android": "EXPO_USE_METRO_WORKSPACE_ROOT=1 expo start --android",
    "serve:ios": "EXPO_USE_METRO_WORKSPACE_ROOT=1 expo start --ios",
    "serve:any": "EXPO_USE_METRO_WORKSPACE_ROOT=1 expo start",
    "serve:web": "EXPO_USE_METRO_WORKSPACE_ROOT=1 expo start --web",
    "eject": "expo eject",
    "format": "eslint . --ext .ts,.tsx --fix",
    "lint": "tsc --noEmit && eslint . --ext .ts,.tsx",
    "test": "LOG_LEVEL=\"${LOG_LEVEL:-off}\" jest src/ --runInBand --passWithNoTests",
    "test:watch": "LOG_LEVEL=\"${LOG_LEVEL:-off}\" jest src/ --watch --runInBand --passWithNoTests"
  },
  "dependencies": {
    "@parker/core-client": "*",
    "@react-native-async-storage/async-storage": "^1.13.0",
    "axios": "^1.7.5",
    "expo": "^51.0.31",
    "expo-constants": "^16.0.2",
    "expo-linking": "~6.3.1",
    "expo-router": "^3.5.23",
    "expo-secure-store": "~13.0.2",
    "expo-status-bar": "~1.12.1",
    "nativewind": "^2.0.11",
    "prop-types": "^15.8.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.75.2",
    "react-native-flash-message": "^0.4.2",
    "react-native-gesture-handler": "~2.18.1",
    "react-native-paper": "^5.12.5",
    "react-native-safe-area-context": "4.10.9",
    "react-native-screens": "~3.34.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-vector-icons": "^10.1.0",
    "react-use": "^17.5.1",
    "supertokens-react-native": "^5.1.2",
    "tslib": "^2.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@types/react": "^18.3.4",
    "@types/react-native": "^0.73.0",
    "eslint": "^8.57.0",
    "eslint-config-custom": "*",
    "jest": "^29.7.0",
    "tailwindcss": "3.4.10",
    "ts-jest": "^29.2.5",
    "typescript": "^5.5.4"
  }
}
