{
  "name": "@parker/landlord",
  "version": "0.0.1",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "clean": "rm -rf .expo/ .turbo/ dist/ coverage/ node_modules/",
    "serve": "yarn serve:ios",
    "serve:android": "EXPO_USE_METRO_WORKSPACE_ROOT=1 expo start --android",
    "serve:ios": "EXPO_USE_METRO_WORKSPACE_ROOT=1 expo start --ios",
    "serve:any": "EXPO_USE_METRO_WORKSPACE_ROOT=1 expo start",
    "serve:web": "EXPO_USE_METRO_WORKSPACE_ROOT=1 expo start --web",
    "eject": "expo eject",
    "format": "eslint . --ext .ts,.tsx --fix",
    "lint": "tsc --noEmit && eslint . --ext .ts,.tsx",
    "test": "LOG_LEVEL=\"${LOG_LEVEL:-off}\" jest src/ --runInBand --passWithNoTests",
    "test:watch": "LOG_LEVEL=\"${LOG_LEVEL:-off}\" jest src/ --watch --runInBand --passWithNoTests"
  },
  "dependencies": {
    "@parker/core-client": "*",
    "@react-native-async-storage/async-storage": "1.18.2",
    "axios": "^1.5.0",
    "expo": "^49.0.11",
    "expo-constants": "^14.4.2",
    "expo-linking": "~5.0.2",
    "expo-router": "^2.0.8",
    "expo-secure-store": "~12.3.1",
    "expo-status-bar": "~1.6.0",
    "nativewind": "^2.0.11",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.6",
    "react-native-flash-message": "^0.4.2",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-paper": "^5.11.1",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-use": "^17.4.0",
    "supertokens-react-native": "^4.0.8",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.22.15",
    "@types/react": "^18.0.22",
    "@types/react-native": "^0.72.2",
    "eslint": "8.49.0",
    "eslint-config-custom": "*",
    "jest": "^29.0.3",
    "tailwindcss": "3.3.2",
    "ts-jest": "^29.0.3",
    "typescript": "^5.2.2"
  }
}
