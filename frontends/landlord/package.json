{
  "name": "@parker/landlord",
  "version": "0.0.1",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "clean": "rm -rf .expo/ .turbo/ dist/ coverage/ node_modules/",
    "serve": "yarn serve:ios",
    "serve:android": "expo start --android",
    "serve:ios": "expo start --ios",
    "serve:any": "expo start",
    "serve:web": "expo start --web",
    "eject": "expo eject",
    "format": "eslint \"src/**\" --fix",
    "lint": "tsc --noEmit && eslint \"src/**\"",
    "test": "LOG_LEVEL=\"${LOG_LEVEL:-off}\" jest --forceExit --runInBand --passWithNoTests src/",
    "test:watch": "LOG_LEVEL=\"${LOG_LEVEL:-off}\" jest --forceExit --runInBand --passWithNoTests --watch src/"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@parker/errors": "*",
    "@parker/parking-client": "*",
    "@parker/places-client": "*",
    "@react-native-async-storage/async-storage": "1.23.1",
    "axios": "^1.11.0",
    "expo": "~52.0.0",
    "expo-constants": "~17.0.0",
    "expo-linking": "~7.0.0",
    "expo-localization": "~16.0.0",
    "expo-location": "~18.0.0",
    "expo-router": "~4.0.0",
    "expo-secure-store": "~14.0.0",
    "expo-status-bar": "~2.0.0",
    "nativewind": "^4.1.23",
    "prop-types": "^15.8.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.62.0",
    "react-native": "0.76.9",
    "react-native-flash-message": "^0.4.2",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "4.4.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-use": "^17.6.0",
    "supertokens-react-native": "^5.1.5",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@parker/eslint-config": "*",
    "@parker/test-utils": "*",
    "@types/react": "~18.3.0",
    "eslint": "^9.34.0",
    "eslint-plugin-no-relative-import-paths": "^1.6.1",
    "eslint-plugin-react": "^7.37.5",
    "jest": "~29.7.0",
    "tailwindcss": "~3.4.0",
    "ts-jest": "^29.4.1",
    "typescript": "~5.9.2"
  }
}
