{
  "name": "@parker/parker-landlord",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf node_modules/ dist/ coverage/ .turbo/",
    "dev": "yarn dev:ios",
    "dev:android": "expo start --android",
    "dev:ios": "expo start --ios",
    "dev:start": "expo start",
    "dev:web": "expo start --web",
    "eject": "expo eject",
    "format": "eslint . --ext .ts --fix",
    "lint": "tsc --noEmit && eslint . --ext .ts",
    "test": "LOG_LEVEL=off jest src/ --runInBand --passWithNoTests",
    "test:watch": "LOG_LEVEL=off jest src/ --watch --runInBand --passWithNoTests"
  },
  "dependencies": {
    "expo": "^47.0.8",
    "expo-status-bar": "^1.4.2",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-native": "0.70.5",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@types/react": "^18.0.22",
    "@types/react-native": "^0.69.15",
    "eslint": "7.32.0",
    "eslint-config-custom": "*",
    "jest": "^29.0.3",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.3"
  },
  "private": true
}
