{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "clean": {
      "cache": false
    },
    "db:migrate-up": {
      "cache": false
    },
    "db:migrate-up:test": {
      "cache": false
    },
    "dev": {
      "cache": false
    },
    "format": {
      "outputs": []
    },
    "generate": {
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "outputs": []
    },
    "test:watch": {
      "cache": false
    }
  }
}
