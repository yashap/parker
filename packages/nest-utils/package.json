{
  "name": "@parker/nest-utils",
  "version": "0.0.1",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf node_modules/ dist/ coverage/ .turbo/",
    "format": "eslint . --ext .ts --fix",
    "lint": "tsc --noEmit && eslint . --ext .ts",
    "test": "LOG_LEVEL=\"${LOG_LEVEL:-off}\" jest src/",
    "test:watch": "LOG_LEVEL=\"${LOG_LEVEL:-off}\" jest --watch src/"
  },
  "dependencies": {
    "@parker/logging": "*",
    "@nestjs/common": "^9.1.6",
    "@nestjs/core": "^9.1.6",
    "express": "^4.18.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.7",
    "tslib": "^2.4.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@parker/errors": "*",
    "@parker/tsconfig": "*",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.0.0",
    "@types/node": "^17.0.12",
    "@types/uuid": "^8.3.4",
    "eslint": "7.32.0",
    "eslint-config-custom": "*",
    "jest": "^29.0.3",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.3"
  }
}
