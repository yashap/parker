{
  "name": "@parker/nest-utils",
  "version": "0.0.1",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf node_modules/ dist/ coverage/ .turbo/",
    "format": "eslint \"src/**\" --fix",
    "lint": "tsc --noEmit && eslint \"src/**\"",
    "test": "LOG_LEVEL=\"${LOG_LEVEL:-off}\" jest --forceExit --runInBand src/",
    "test:watch": "LOG_LEVEL=\"${LOG_LEVEL:-off}\" jest --forceExit --runInBand --watch src/"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.6",
    "@nestjs/core": "^11.1.6",
    "@parker/api-client-utils": "*",
    "@parker/core": "*",
    "@parker/correlation-id-propagator": "*",
    "@parker/errors": "*",
    "@parker/logging": "*",
    "@ts-rest/core": "^3.52.1",
    "@ts-rest/nest": "^3.52.1",
    "express": "^5.1.0",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "supertokens-node": "^23.0.1",
    "tslib": "^2.8.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@parker/api-client-test-utils": "*",
    "@parker/eslint-config": "*",
    "@parker/test-utils": "*",
    "@parker/tsconfig": "*",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.3.0",
    "@types/uuid": "^10.0.0",
    "eslint": "^9.34.0",
    "jest": "~29.7.0",
    "ts-jest": "^29.4.1",
    "typescript": "~5.9.2",
    "zod": "^3.24.1"
  }
}
