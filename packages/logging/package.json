{
  "name": "@parker/logging",
  "version": "0.0.1",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf node_modules/ dist/ coverage/ .turbo/",
    "format": "eslint . --ext .ts --fix",
    "lint": "tsc --noEmit && eslint . --ext .ts",
    "test": "LOG_LEVEL=off jest src/",
    "test:watch": "LOG_LEVEL=off jest --watch src/"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "tslib": "^2.4.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@parker/context-propagation": "*",
    "@parker/tsconfig": "*",
    "@types/jest": "^29.0.0",
    "@types/lodash": "^4.14.187",
    "@types/node": "^17.0.12",
    "eslint": "7.32.0",
    "eslint-config-custom": "*",
    "jest": "^29.0.3",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.3"
  }
}
