{
  "name": "@parker/core-client",
  "version": "0.0.1",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf node_modules/ dist/ coverage/ .turbo/",
    "format": "eslint . --ext .ts --fix",
    "generate": "npx openapi-generator-cli generate -i=core-service.yaml -g=typescript-axios -o=src/generated --model-name-suffix=Dto --additional-properties=withSeparateModelsAndApi=true,apiPackage=api,modelPackage=model",
    "lint": "tsc --noEmit && eslint . --ext .ts"
  },
  "dependencies": {
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "@parker/axios-utils": "*",
    "axios": "^1.3.2"
  },
  "devDependencies": {
    "@parker/tsconfig": "*",
    "@types/node": "^17.0.12",
    "eslint": "7.32.0",
    "eslint-config-custom": "*",
    "typescript": "^4.9.3"
  }
}
