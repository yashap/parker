{
  "name": "@parker/parking",
  "version": "0.0.1",
  "private": true,
  "main": "./src/main.ts",
  "types": "./src/main.ts",
  "scripts": {
    "build": "nest build",
    "clean": "rm -rf node_modules/ dist/ coverage/ .turbo/",
    "db:clean": "yarn db:up && ../../tools/scripts/drop_db.sh parking parker_postgres_dev && ../../tools/scripts/drop_user.sh parking parker_postgres_dev",
    "db:clean:test": "yarn db:up:test && ../../tools/scripts/drop_db.sh parking parker_postgres_test && ../../tools/scripts/drop_user.sh parking parker_postgres_test",
    "db:dump-fixtures": "../../tools/scripts/dump_fixtures.sh parking",
    "db:generate-custom-migration": "../../tools/scripts/generate_custom_db_migration.sh",
    "db:generate-migration": "yarn db:up && ../../tools/scripts/generate_db_migration.sh",
    "db:migrate-up": "yarn db:up && DATABASE_URL='postgresql://dev_admin:dev_admin_password@localhost:5440/parking?schema=public' yarn drizzle-kit migrate",
    "db:migrate-up:test": "yarn db:up:test && DATABASE_URL='postgresql://dev_admin:dev_admin_password@localhost:5441/parking?schema=public' yarn drizzle-kit migrate",
    "db:restore-fixtures": "yarn db:clean && yarn db:up && ../../tools/scripts/restore_fixtures.sh parking && yarn db:migrate-up",
    "db:up": "../../tools/scripts/ensure_postgres_db.sh -d parking && ../../tools/scripts/ensure_postgres_user.sh -u parking -w parking_password -d parking",
    "db:up:test": "../../tools/scripts/ensure_postgres_db.sh -d parking -c parker_postgres_test && ../../tools/scripts/ensure_postgres_user.sh -u parking -w parking_password -d parking -c parker_postgres_test",
    "serve": "nest start --watch",
    "format": "eslint \"src/**\" --fix",
    "lint": "tsc --noEmit && eslint \"src/**\"",
    "test": "LOG_LEVEL=\"${LOG_LEVEL:-off}\" jest --forceExit --runInBand src/",
    "test:watch": "LOG_LEVEL=\"${LOG_LEVEL:-off}\" jest --forceExit --runInBand --watch src/"
  },
  "dependencies": {
    "@js-temporal/polyfill": "^0.5.1",
    "@nestjs/common": "^11.1.6",
    "@nestjs/core": "^11.1.6",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/platform-express": "^11.1.6",
    "@parker/api-client-utils": "*",
    "@parker/drizzle-utils": "*",
    "@parker/errors": "*",
    "@parker/geography": "*",
    "@parker/logging": "*",
    "@parker/nest-utils": "*",
    "@parker/parking-client": "*",
    "@parker/time": "*",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dotenv": "^17.2.1",
    "drizzle-orm": "^0.44.5",
    "geo-tz": "^8.1.4",
    "lodash": "^4.17.21",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "supertokens-node": "^23.0.1",
    "tslib": "^2.8.1",
    "uuid": "^11.1.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.7",
    "@nestjs/testing": "^11.1.6",
    "@parker/api-client-test-utils": "*",
    "@parker/eslint-config": "*",
    "@parker/test-utils": "*",
    "@parker/tsconfig": "*",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.3.0",
    "@types/pg": "^8.15.5",
    "@types/uuid": "^10.0.0",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.34.0",
    "eslint-plugin-no-relative-import-paths": "^1.6.1",
    "jest": "~29.7.0",
    "ts-jest": "^29.4.1",
    "typescript": "~5.9.2"
  }
}
